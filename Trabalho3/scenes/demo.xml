<lxs>
    <scene root="root" axis_length="5"/>

    <views default="defaultCamera">
        <perspective id="defaultCamera" near="0.1" far="500" angle="25">
            <from x="30" y="30" z="30" />
            <to x="0" y="0" z="0" />
        </perspective>
        <ortho id="orthoCamera" near="-50" far="300" left="-40" right="40" top="20" bottom="-20" >
            <to x="0" y="0" z="-1" />
            <from x="0" y="0" z="1" />
            <up x="0" y="1" z="1"/> 
        </ortho>
        <perspective id="securityCamera" near="0.1" far="500" angle="70">
            <from x="-0.1" y="0" z="0" />
            <to x="-15" y="2" z="0" />
        </perspective>
    </views>

    <globals>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
        <background r="0.0" g="0.0" b="0.0" a="1.0" />
    </globals>

    <lights>
        <omni id="demoLight" enabled="1">
            <location x="-5" y="5" z="5" w="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <attenuation constant="1.0" linear="0.1" quadratic="0.01" />
        </omni>

        <omni id="light" enabled="1">
            <location x="0" y="0" z="0" w="1.0" />
            <ambient r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <attenuation constant="1.0" linear="0.0" quadratic="0.00" />
        </omni>

        <spot id="spot" enabled="0" angle="30" exponent="2">     
            <location x="0" y="0" z="8" w="1.0" />
            <target x="0" y="0" z="-5" />
            <ambient r="0.0" g="0.0" b="0.0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <attenuation constant="1.0" linear="0.0" quadratic="0.00" />
        </spot>
    </lights>

    <textures>
        <texture id="sun" file="scenes/images/sun.jpg" />
        <texture id="gold" file="scenes/images/golden.jpg" />
        <texture id="vidral" file="scenes/images/vidral.jpg" />
        <texture id="leaves" file="scenes/images/leaves.jpg" />
        <texture id="earth" file="scenes/images/earth.jpg" />
        <texture id="moon" file="scenes/images/moon.jpg" />
        <texture id="head" file="scenes/images/head.jpg" />
        <texture id="astronaut" file="scenes/images/astronaut.jpg" />
        <texture id="rocket" file= "scenes/images/rocket.jpg"/>
        <texture id="flag" file="scenes/images/flag.png" />
        <texture id="space" file= "scenes/images/space.png"/>
    </textures>

    <materials>
        <material id="graydiffusematerial" shininess="4">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.25" g="0.25" b="0.25" a="1.0" />
            <diffuse r="0.8" g="0.8" b="0.8" a="1.0" />
            <specular r="0.5" g="0.5" b="0.5" a="1.0" />
        </material>
        <material id="lunar" shininess="10">
            <emission r="0.1" g="0.1" b="0.1" a="1.0" />
            <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
            <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
            <specular r="0.2" g="0.2" b="0.2" a="1.0" />
        </material>
        <material id="bluediffSpec" shininess="15">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.05" g="0.05" b="0.25" a="1.0" />
            <diffuse r="0.2" g="0.2" b="1.0" a="1.0" />
            <specular r="0.1" g="0.1" b="0.5" a="1.0" />
        </material>
        <material id="orangediffSpec" shininess="20">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.15" g="0.15" b="0.15" a="1.0" />
            <diffuse r="1.0" g="0.6" b="0.0" a="1.0" />
            <specular r="0.5" g="0.3" b="0.0" a="1.0" />
        </material>
        <material id="shinyGreen" shininess="3">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.05" g="0.15" b="0.05" a="1.0" />
            <diffuse r="0.1" g="0.4" b="0.1" a="1.0" />
            <specular r="0.9" g="0.9" b="0.9" a="1.0" />
        </material>
        <material id="greendiff" shininess="3">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.05" g="0.15" b="0.05" a="1.0" />
            <diffuse r="0.1" g="0.4" b="0.1" a="1.0" />
            <specular r="0.1" g="0.4" b="0.1" a="1.0" />
        </material>
        <material id="reddiffSpec" shininess="20">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.15" g="0.1" b="0.1" a="1.0" />
            <diffuse r="0.9" g="0.25" b="0.25" a="1.0" />
            <specular r="0.45" g="0.125" b="0.125" a="1.0" />
        </material>
        <material id="graySpec" shininess="20">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.25" g="0.25" b="0.25" a="1.0" />
            <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
            <specular r="0.8" g="0.8" b="0.8" a="1.0" />
        </material>
        <material id="browndiff" shininess="3">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.1" g="0.05" b="0.025" a="1.0" />
            <diffuse r="0.4" g="0.2" b="0.1" a="1.0" />
            <specular r="0.1" g="0.05" b="0.025" a="1.0" />
        </material>
        <material id="redemissor" shininess="20">
            <emission r="1" g="0.8" b="0.15" a="1.0" />
            <ambient r="0" g="0" b="0" a="1.0" />
            <diffuse r="0.5" g="0.4" b="0" a="1.0" />
            <specular r="0.5" g="0.4" b="0" a="1.0" />
        </material>
        <material id="skindiff" shininess="20">
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.25" g="0.2" b="0.2" a="1.0" />
            <diffuse r="0.9" g="0.9" b="0.9" a="1.0" />
            <specular r="0.5" g="0.4" b="0.4" a="1.0" />
        </material>

    </materials>

    <transformations>
        <transformation id="moontransform1">
            <translate x="2" y="2" z="2"/>
            <scale x="0.5" y="0.5" z="0.5"/>
        </transformation>
        <transformation id="moontransform2">
            <translate x="-2.5" y="-1.5" z="-1"/>
            <scale x="0.5" y="0.5" z="0.5"/>
        </transformation>
        <transformation id="earthmoontransform">
            <translate x="-1.5" y="-0.5" z="3"/>
            <scale x="0.5" y="0.5" z="0.5"/>
        </transformation>
    </transformations>

    <animations>
        <animation id="rocketanimation" >
            <keyframe instant="2.75">
                <translate x="13" y="0" z="0" />
                <rotate angle_x="90" angle_y="0" angle_z="0" />
                <scale x="1" y="1" z="1" />  
            </keyframe>
            <keyframe instant="4">
                <translate x="16" y="0" z="0" />
                <rotate angle_x="120" angle_y="0" angle_z="0" />
                <scale x="1" y="1" z="1" />  
            </keyframe>
            <keyframe instant="5">
                <translate x="17" y="0" z="0" />
                <rotate angle_x="140" angle_y="0" angle_z="0" />
                <scale x="1" y="1" z="1" />  
            </keyframe>
            <keyframe instant="6.25">
                <translate x="18" y="0" z="0" />
                <rotate angle_x="160" angle_y="0" angle_z="0" />
                <scale x="1" y="1" z="1" />  
            </keyframe>
            <keyframe instant="7.5">
                <translate x="18" y="0" z="0" />
                <rotate angle_x="160" angle_y="0" angle_z="0" />
                <scale x="1" y="1" z="1" />  
            </keyframe>
            <keyframe instant="10.5">
                <translate x="10" y="0" z="0" />
                <rotate angle_x="180" angle_y="0" angle_z="0" />
                <scale x="1" y="1" z="1" />  
            </keyframe>
            <keyframe instant="11">
                <translate x="10" y="0" z="0" />
                <rotate angle_x="180" angle_y="0" angle_z="0" />
                <scale x="1" y="1" z="1" />  
            </keyframe>
            <keyframe instant="11.20">
                <translate x="-50" y="0" z="0" />
                <rotate angle_x="180" angle_y="0" angle_z="0" />
                <scale x="1" y="1" z="1" />  
            </keyframe>
        </animation>
        <animation id="personanimation" >
            <keyframe instant="7.5">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0"/>
                <scale x="1" y="1" z="1" />  
            </keyframe>
            <keyframe instant="10.5">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0"/>
                <scale x="2" y="2" z="2" />  
            </keyframe>
            <keyframe instant="12">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0"/>
                <scale x="2" y="2" z="2" />  
            </keyframe>
            <keyframe instant="14">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="720" angle_z="0"/>
                <scale x="1.5" y="1.5" z="1.5" />  
            </keyframe>
            <keyframe instant="16">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="2160" angle_z="0"/>
                <scale x="1.5" y="1.5" z="1.5" />  
            </keyframe>
            <keyframe instant="26">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="12960" angle_z="0"/>
                <scale x="1.5" y="1.5" z="1.5" />  
            </keyframe>
        </animation>
        <animation id="solarsystemanimation" >
            <keyframe instant="14">
                <translate x="0" y="0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0"/>
                <scale x="1" y="1" z="1" />  
            </keyframe>
            <keyframe instant="16">
                <translate x="-12.5" y="0" z="-5" />
                <rotate angle_x="0" angle_y="0" angle_z="0"/>
                <scale x="0.5" y="0.5" z="0.5" />  
            </keyframe>
            <keyframe instant="20">
                <translate x="-25" y="0" z="-10" />
                <rotate angle_x="0" angle_y="0" angle_z="0"/>
                <scale x="0.01" y="0.01" z="0.01" />  
            </keyframe>
            <keyframe instant="21">
                <translate x="-6" y="0" z="0" />
                <rotate angle_x="0" angle_y="0" angle_z="0"/>
                <scale x="0" y="0" z="0" />  
            </keyframe>
        </animation>
    </animations>

    <primitives>
        <primitive id="plane">
            <plane npartsU="5" npartsV="5"/>
        </primitive>

        <primitive id="unitsquare">
            <rectangle x1="-0.5" y1="-0.5" x2="0.5" y2="0.5" />
        </primitive>
        <primitive id="triangle">
            <triangle x1="-0.5" y1="0" z1="0"
                      x2="0.5" y2="0" z2="0"
                      x3="0" y3="0.7071" z3="0"/> 
        </primitive>
        <primitive id="cylinder1">
            <cylinder base="2" top="2" height="6" slices="20" stacks="20" />
        </primitive>
         <primitive id="cylinder2">
            <cylinder2 base="2" top="0.3" height="6" slices="20" stacks="20" />
        </primitive>
        <primitive id="cone1">
            <cylinder base="2" top="0" height="4" slices="20" stacks="6" />
        </primitive>
        <primitive id="cone2">
            <cylinder base="0.5" top="1.0" height="2" slices="16" stacks="6" />
        </primitive>
        <primitive id="torusring">
            <torus inner="0.15" outer="1" slices="8" loops="32" />
        </primitive>
        <primitive id="sphere">
            <sphere radius="1" slices="32" stacks="32" />
        </primitive>
        <primitive id="cylinder">
            <cylinder base="1" top="1" height="1" slices="12" stacks="12" />
        </primitive>
        <primitive id="cone">
            <cylinder base="1" top="0" height="1" slices="12" stacks="12" />
        </primitive>
        <primitive id="patch1">
            <patch npointsU="3" npointsV="4" npartsU="16" npartsV="16" >
                <controlpoint xx="-2" yy="-2" zz="1" />
                <controlpoint xx="-2" yy="-1" zz="-2" />
                <controlpoint xx="-2" yy="1" zz="5" />
                <controlpoint xx="-2" yy="2" zz="-1" />

                <controlpoint xx="0" yy="-2" zz="0" />
                <controlpoint xx="0" yy="-1" zz="-1" />
                <controlpoint xx="0" yy="1" zz="1.5" />
                <controlpoint xx="0" yy="2" zz="0" />

                <controlpoint xx="2" yy="-2" zz="-1" />
                <controlpoint xx="2" yy="-1" zz="2" />
                <controlpoint xx="2" yy="1" zz="-5" />
                <controlpoint xx="2" yy="2" zz="1" />
            </patch>
        </primitive>
    </primitives>

    <components>
        <component id="root">
            <transformation/>
            <materials>
                <material id="graydiffusematerial" />
            </materials>
            <texture id="none"/>
            <children> 
                <componentref id="solarsystem"/>
                <componentref id="person"/>
                <componentref id="rocket"/>
                <componentref id="skybox"/>
            </children>
        </component>

        <component id="solarsystem">
            <transformation/>
            <materials>
                <material id="graydiffusematerial" />
            </materials>
            <texture id="none"/>
            <children> 
                <componentref id="pyramidplanet"/>
                <componentref id="planet1"/>
                <componentref id="planet2"/>
                <componentref id="planet3"/>
                <componentref id="planet4"/>
                <componentref id="planet5"/>
                <componentref id="planet6"/>
                <componentref id="planet7"/>
                <componentref id="earth"/>
                <componentref id="sun"/>
            </children>
            <animationref id="solarsystemanimation"/>
        </component>

        <component id="skybox">
            <transformation>
                <scale x="150" y="150" z="150"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="space"/>
            <children>
                <componentref id="skyboxtop"/>
                <componentref id="skyboxbot"/>
                <componentref id="skyboxleft"/>
                <componentref id="skyboxright"/>
                <componentref id="skyboxfront"/>
                <componentref id="skyboxback"/>
            </children>
        </component>

        <component id="skyboxtop">
            <transformation>
                <translate x="0" y="0.5" z="0"/>
                <rotate axis="z" angle="180"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <primitiveref id="plane"/>
            </children>
        </component>

        <component id="skyboxbot">
            <transformation>
                <translate x="0" y="-0.5" z="0"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <primitiveref id="plane"/>
            </children>
        </component>

        <component id="skyboxleft">
            <transformation>
                <translate x="-0.5" y="0" z="0"/>
                <rotate axis="z" angle="-90"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <primitiveref id="plane"/>
            </children>
        </component>

        <component id="skyboxright">
            <transformation>
                <translate x="0.5" y="0" z="0"/>
                <rotate axis="z" angle="90"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <primitiveref id="plane"/>
            </children>
        </component>

        <component id="skyboxfront">
            <transformation>
                <translate x="0" y="0" z="0.5"/>
                <rotate axis="x" angle="-90"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <primitiveref id="plane"/>
            </children>
        </component>

        <component id="skyboxback">
            <transformation>
                <translate x="0" y="0" z="-0.5"/>
                <rotate axis="x" angle="90"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <primitiveref id="plane"/>
            </children>
        </component>


        <component id="pyramidplanet">
            <transformation>
                <translate x="-8" y="2" z="-5"/>
            </transformation>
            <materials>
                <material id="shinyGreen" />
            </materials>
            <texture id="vidral" length_s="0.33" length_t="0.33"/>
            <children>
                <componentref id="diamond"/>
                <componentref id="ring"/>
            </children>
        </component>

        <component id="diamond">
            <transformation>
                <scale x="2" y="2" z="2"/>
                <rotate axis="x" angle="90"/>
                <rotate axis="y" angle="45"/>
             </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <componentref id="pyramid"/>
                <componentref id="pyramiddown"/>
            </children>
        </component>

        <component id="pyramiddown">
            <transformation>
                <rotate axis="x" angle="180"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <componentref id="pyramid"/>
            </children>
        </component>

        <component id="pyramid">
            <transformation/>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <componentref id="pyramidright"/>
                <componentref id="pyramidleft"/>
                <componentref id="pyramidfront"/>
                <componentref id="pyramidback"/>
                <componentref id="pyramidbot"/>
            </children>
        </component>

        <component id="pyramidfront">
            <transformation>
                <translate x="0" y="0" z="0.5"/>
                <rotate axis="x" angle="-45"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <componentref id="pyramidside"/>
            </children>
        </component>

        <component id="pyramidback">
            <transformation>
                <translate x="0" y="0" z="-0.5"/>
                <rotate axis="x" angle="45"/>
                <rotate axis="y" angle="180"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <componentref id="pyramidside"/>
            </children>
        </component>

        <component id="pyramidleft">
            <transformation>
                <translate x="-0.5" y="0" z="0"/>
                <rotate axis="z" angle="-45"/>
                <rotate axis="y" angle="-90"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <componentref id="pyramidside"/>
            </children>
        </component>

        <component id="pyramidright">
            <transformation>
                <translate x="0.5" y="0" z="0"/>
                <rotate axis="z" angle="45"/>
                <rotate axis="y" angle="90"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <componentref id="pyramidside"/>
            </children>
        </component>

        <component id="pyramidbot">
            <transformation>
                <rotate axis="x" angle="90"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <primitiveref id="unitsquare"/>
            </children>
        </component>

        <component id="pyramidside">
            <transformation/>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <primitiveref id="triangle"/>
            </children>
        </component>

        <component id="person">
            <transformation>
                <translate x="-25" y="0" z="-10"/>
                <rotate axis="y" angle="45"/>
                <rotate axis="z" angle="45"/>
                <rotate axis="x" angle="25"/>
                <scale x="5" y="5" z="5"/>
            </transformation>
            <materials>
                <material id="inherit" />
            </materials>
            <texture id="inherit"/>
            <children>
                <componentref id="hat"/>
                <componentref id="head"/>
                <componentref id="body"/>
                <componentref id="flagStructure"/>
            </children>
            <animationref id="personanimation"/>
        </component>

       <component id="hat">
            <transformation>
                <translate x="0" y="3" z="0"/>
                <scale x="0.4" y="0.4" z="0.4"/>
                <rotate axis="x" angle="-90"/>
            </transformation>
            <materials>
                <material id="inherit"/>
            </materials>
            <texture id="inherit"/>
            <children>
                <componentref id="hattop"/>
                <componentref id="hatbody"/>
                <componentref id="hatbot"/> 
            </children>
        </component>
        <component id="hattop">
            <transformation>
                <translate x="0" y="0" z="6.5"/>
                <scale x="0.6" y="0.6" z="0.6"/>
            </transformation>
            <materials>
                <material id="greendiff"/>
            </materials>
            <texture id="inherit"/>
            <children>
                <primitiveref id="sphere"/>
            </children>
        </component>
